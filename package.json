{
  "name": "labs-back",
  "version": "0.3.0",
  "description": "Api de funcionalidad base de Laboratorios San Francisco",
  "author": {
    "email": "contacto@xst.mx",
    "name": "Xquenda Soluciones Tecnológicas S.A. de C.V.",
    "url": "https://xst.mx",
    "contact": "9515042982"
  },
  "private": true,
  "license": "Copyright - 2021",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --max-warnings 0",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --max-warnings 0 --fix",
    "test": "jest --config ./test/jest-e2e.json --detectOpenHandles --forceExit --runInBand",
    "typeorm": "node --require ts-node/register -r ./tsconfig-paths-bootstrap.ts ./node_modules/typeorm/cli.js",
    "mig:gen": "ts-node -r ./tsconfig-paths-bootstrap.ts ./node_modules/typeorm/cli.js migration:generate --config orm.config.json --name",
    "mig:run": "ts-node -r ./tsconfig-paths-bootstrap.ts ./node_modules/typeorm/cli.js migration:run --config orm.config.json",
    "mig:rev": "ts-node -r ./tsconfig-paths-bootstrap.ts ./node_modules/typeorm/cli.js migration:revert --config orm.config.json",
    "doc": "compodoc -p tsconfig.build.json -s -w --theme material --hideGenerator -o --customLogo src/assets/logo.webp -n \"Documentación\"",
    "doc:gen": "compodoc -p tsconfig.build.json --theme material --hideGenerator --customLogo src/assets/logo.webp -n \"Documentación\"",
    "doc:cov": "compodoc -p tsconfig.build.json --coverageMinimumPerFile 50 --coverageTest 80",
    "precommit": "echo 'Running all tests...' && npm run truncate && npm run lint && npm run doc:cov && npm run build && npm run test:cov && echo 'Congratulations your commit will pass the pipeline!'&&exit 0",
    "truncate": "bash truncate-tables.sh",
    "drop": "bash drop-tables.sh",
    "X": "npm run drop && npm run mig:run && npm run test",
    "Xdev": "npm run drop && npm run mig:run && npm run start:dev",
    "XX": "npm run truncate && npm run test",
    "seed": "node_modules/.bin/ts-node -r ./tsconfig-paths-bootstrap.ts src/app.seeder.ts",
    "seed:refresh": "node_modules/.bin/ts-node -r ./tsconfig-paths-bootstrap.ts src/app.seeder --refresh"
  },
